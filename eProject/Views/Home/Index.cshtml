@{
    ViewData["Title"] = "Home";
}
@using eProject.Models

<!-- PAGE -->
@if (ViewBag.isHome != null)
{
    @await Component.InvokeAsync("SlideShow")
}

<!-- /PAGE -->
<!-- PAGE -->
<section class="page-section">
    <div class="container">

        <div class="tabs">
            <ul id="tabs" class="nav nav-justified-off">
                <li class="active"><a href="#tab-1" data-toggle="tab">Featured</a></li>
                <li class=""><a href="#tab-2" data-toggle="tab">Newest</a></li>
            </ul>
        </div>

        <div class="tab-content">

            <!-- tab 1 -->
            <div class="tab-pane fade active in" id="tab-1">
                <div class="row">
                    @foreach (var featuredProduct in ViewBag.FeaturedProducts)
                    {
                        Product product = (Product)featuredProduct;
                        Photo photo = product.Photos.SingleOrDefault(ph => ph.Status && ph.Featured);
                        var photoName = photo == null ? "no-image.jpg" : photo.Name;
                        <div class="col-md-3 col-sm-6">
                            <div class="thumbnail no-border no-padding">
                                <div class="media">
                                    <a class="media-link" data-gal="prettyPhoto" href="~/products/@photoName">
                                        <img src="~/products/@photoName" alt="" />
                                        <span class="icon-view"><strong><i class="fa fa-eye"></i></strong></span>
                                    </a>
                                </div>
                                <div class="caption text-center">
                                    <h4 class="caption-title"><a asp-controller="Product" asp-action="details" asp-route-id="@featuredProduct.ProductId">@featuredProduct.Name</a></h4>
                                    <div class="rating">
                                        <span class="star"></span>
                                        <!--
                                        -->
                                        <span class="star active"></span>
                                        <!--
                                        -->
                                        <span class="star active"></span>
                                        <!--
                                        -->
                                        <span class="star active"></span>
                                        <!--
                                        -->
                                        <span class="star active"></span>
                                    </div>
                                    <div class="price"><ins>@featuredProduct.Price</ins> <del>$425.00</del></div>
                                    <div class="buttons">
                                        <a class="btn btn-theme btn-theme-transparent btn-wish-list" href="#"><i class="fa fa-heart"></i></a>
                                        <!--
                                        -->
                                        <a class="btn btn-theme btn-theme-transparent btn-icon-left" href="#"><i class="fa fa-shopping-cart"></i>Add to Cart</a>
                                        <!--
                                        -->
                                        <a class="btn btn-theme btn-theme-transparent btn-compare" href="#"><i class="fa fa-exchange"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>

            <!-- tab 2 -->
            <div class="tab-pane fade" id="tab-2">
                <div class="row">
                    @foreach (var newestProduct in ViewBag.NewestProducts)
                    {
                        Product product = (Product)newestProduct;
                        Photo photo = product.Photos.SingleOrDefault(ph => ph.Status && ph.Featured);
                        var photoName = photo == null ? "no-image.jpg" : photo.Name;
                        <div class="col-md-3 col-sm-6">
                            <div class="thumbnail no-border no-padding">
                                <div class="media">
                                    <a class="media-link" data-gal="prettyPhoto" href="~/products/@photoName">
                                        <img src="~/products/@photoName" alt="" />
                                        <span class="icon-view"><strong><i class="fa fa-eye"></i></strong></span>
                                    </a>
                                </div>
                                <div class="caption text-center">
                                    <h4 class="caption-title"><a asp-controller="Product" asp-action="details" asp-route-id="@newestProduct.ProductId">@newestProduct.Name</a></h4>
                                    <div class="rating">
                                        <span class="star"></span>
                                        <!--
                        -->
                                        <span class="star active"></span>
                                        <!--
                        -->
                                        <span class="star active"></span>
                                        <!--
                        -->
                                        <span class="star active"></span>
                                        <!--
                        -->
                                        <span class="star active"></span>
                                    </div>
                                    <div class="price"><ins>@newestProduct.Price</ins> <del>$425.00</del></div>
                                    <div class="buttons">
                                        <a class="btn btn-theme btn-theme-transparent btn-wish-list" href="#"><i class="fa fa-heart"></i></a>
                                        <!--
                        -->
                                        <a class="btn btn-theme btn-theme-transparent btn-icon-left" href="#"><i class="fa fa-shopping-cart"></i>Add to Cart</a>
                                        <!--
                        -->
                                        <a class="btn btn-theme btn-theme-transparent btn-compare" href="#"><i class="fa fa-exchange"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                    </div>
                   
            </div>


        </div>

    </div>
</section>
<!-- /PAGE -->
<!-- PAGE -->
